super + Return
	st
super + control + Return
	st -f "Cozette:pixelsize=32:style=Regular:antialias=false:autohint=true"
super + shift + Return
	xterm
super + e
	emacsclient -nc

super + @space
	dmenu_run
super + Escape
	slock

super + backslash
	mpc toggle
super + bracketleft
	mpc prev
super + bracketright
	mpc next


super + shift + backslash
	playerctl play-pause

XF86AudioMute
	wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle
XF86AudioLowerVolume
	wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-
XF86AudioRaiseVolume
	wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+

XF86MonBrightnessDown
	brightnessctl set 5%-
XF86MonBrightnessUp
	brightnessctl set 5%+
